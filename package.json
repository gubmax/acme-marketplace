{
	"name": "acme-marketplace",
	"private": true,
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"node": ">=20.0.0",
		"pnpm": ">=8.9.0"
	},
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"codegen": "turbo run codegen",
		"lint": "turbo run lint",
		"sb": "turbo run sb",
		"sb.build": "turbo run sb.build",
		"format": "prettier --write **/*.{*js,ts*,json,css} && turbo run format",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "18.4.3",
		"@commitlint/config-conventional": "18.4.3",
		"eslint": "8.56.0",
		"eslint-define-config": "2.0.0",
		"husky": "8.0.3",
		"npm-check-updates": "16.14.12",
		"prettier": "3.1.1",
		"turbo": "1.11.2",
		"typescript": "5.3.3"
	}
}
