{
	"name": "storage",
	"type": "module",
	"version": "0.0.0",
	"scripts": {
		"dev": "FORCE_COLOR=1 tsx watch --clear-screen=false src/main.ts",
		"build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"codegen": "prisma generate",
		"prisma": "tsx scripts/prisma",
		"lint": "pnpm run /^lint./",
		"lint.es": "eslint .",
		"lint.types": "tsc --noEmit",
		"format": "prisma format"
	},
	"dependencies": {
		"@prisma/client": "5.7.0",
		"awilix": "9.0.0",
		"dotenv": "16.3.1",
		"envalid": "8.0.0",
		"fastify": "4.25.1",
		"hyperid": "3.1.1",
		"prisma": "5.7.0",
		"shared": "workspace:*"
	},
	"devDependencies": {
		"@types/node": "20.8.8",
		"dotenv-cli": "7.3.0",
		"eslint-config-custom": "workspace:*",
		"tsc-alias": "1.8.8",
		"tsconfig": "workspace:*",
		"tsx": "4.6.2"
	}
}
