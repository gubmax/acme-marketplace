{
	"name": "marketplace",
	"type": "module",
	"version": "0.0.0",
	"scripts": {
		"dev": "FORCE_COLOR=1 tsx watch --clear-screen=false --ignore *.timestamp-* ./server/main.ts",
		"build": "pnpm run /^build./ && pnpm prerender",
		"build.client": "vite build",
		"build.render": "vite build --ssr",
		"build.server": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"prerender": "tsx scripts/prerender",
		"lint": "pnpm run /^lint./",
		"lint.es": "eslint **/*.ts* --fix",
		"lint.types.client": "tsc -p tsconfig.web.json --noEmit",
		"lint.types.server": "tsc --noEmit",
		"lint.styles": "stylelint client/**/*.css"
	},
	"dependencies": {
		"@fastify/static": "6.10.2",
		"@nanostores/react": "0.7.1",
		"awilix": "8.0.1",
		"envalid": "7.3.1",
		"esbuild": "0.18.17",
		"fastify": "4.21.0",
		"hyperid": "3.1.1",
		"isbot": "3.6.13",
		"nanostores": "0.9.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rxjs": "7.8.1",
		"shared": "workspace:*",
		"ui": "workspace:*",
		"web-vitals": "3.4.0"
	},
	"devDependencies": {
		"@fastify/middie": "8.3.0",
		"@types/node": "18.11.9",
		"@types/react": "18.2.20",
		"@types/react-dom": "18.2.7",
		"@unocss/preset-mini": "0.55.0",
		"@unocss/reset": "0.55.0",
		"@vitejs/plugin-react": "4.0.4",
		"eslint-config-custom": "workspace:*",
		"picocolors": "1.0.0",
		"stylelint": "15.10.2",
		"tsc-alias": "1.8.7",
		"tsconfig": "workspace:*",
		"tsx": "3.12.7",
		"unocss": "0.55.0",
		"vite": "4.4.9",
		"vite-tsconfig-paths": "4.2.0"
	}
}
