{
	"name": "marketplace",
	"type": "module",
	"version": "0.0.0",
	"scripts": {
		"dev": "tsx watch --clear-screen=false --ignore *.timestamp-* ./server/main.ts",
		"build": "pnpm run /^build./ && node dist/scripts/prerender.js",
		"build.client": "vite build",
		"build.render": "vite build --ssr",
		"build.server": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"test": "vitest run",
		"test.related": "vitest related --run",
		"test.watch": "vitest watch",
		"lint": "pnpm run /^lint./",
		"lint.es": "eslint .",
		"lint.types.client": "tsc -p tsconfig.web.json --noEmit",
		"lint.types.server": "tsc --noEmit",
		"lint.styles": "stylelint client/**/*.css"
	},
	"dependencies": {
		"@acme/shared": "workspace:*",
		"@acme/ui": "workspace:*",
		"@fastify/static": "6.12.0",
		"awilix": "9.0.0",
		"envalid": "8.0.0",
		"fastify": "4.25.2",
		"hyperid": "3.1.1",
		"immer": "10.0.3",
		"isbot": "4.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rxjs": "7.8.1",
		"swr": "2.2.4",
		"valibot": "0.25.0",
		"web-vitals": "3.5.0"
	},
	"devDependencies": {
		"@acme/browserslist-config": "workspace:*",
		"@acme/eslint-config": "workspace:*",
		"@acme/typescript-config": "workspace:*",
		"@fastify/middie": "8.3.0",
		"@testing-library/react": "14.1.2",
		"@types/node": "20.8.8",
		"@types/react": "18.2.45",
		"@types/react-dom": "18.2.18",
		"@unocss/preset-mini": "0.58.0",
		"@unocss/reset": "0.58.0",
		"@vitejs/plugin-react-swc": "3.5.0",
		"happy-dom": "12.10.3",
		"picocolors": "1.0.0",
		"stylelint": "16.1.0",
		"tsc-alias": "1.8.8",
		"tsx": "4.7.0",
		"unocss": "0.58.0",
		"vite": "5.0.10",
		"vite-plugin-pwa": "0.17.4",
		"vite-tsconfig-paths": "4.2.2",
		"vitest": "1.1.0",
		"workbox-window": "7.0.0"
	}
}
