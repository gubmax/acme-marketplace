{
	"name": "marketplace",
	"type": "module",
	"version": "0.0.0",
	"scripts": {
		"dev": "FORCE_COLOR=1 tsx watch --clear-screen=false --ignore *.timestamp-* ./server/main.ts",
		"build": "pnpm run /^build./ && node dist/scripts/prerender.js",
		"build.client": "vite build",
		"build.render": "vite build --ssr",
		"build.server": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
		"lint": "pnpm run /^lint./",
		"lint.es": "eslint .",
		"lint.types.client": "tsc -p tsconfig.web.json --noEmit",
		"lint.types.server": "tsc --noEmit",
		"lint.styles": "stylelint client/**/*.css"
	},
	"dependencies": {
		"@fastify/static": "6.12.0",
		"awilix": "9.0.0",
		"envalid": "8.0.0",
		"fastify": "4.25.1",
		"hyperid": "3.1.1",
		"immer": "10.0.3",
		"isbot": "4.1.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rxjs": "7.8.1",
		"shared": "workspace:*",
		"swr": "2.2.4",
		"ui": "workspace:*",
		"web-vitals": "3.5.0"
	},
	"devDependencies": {
		"@fastify/middie": "8.3.0",
		"@types/node": "20.8.8",
		"@types/react": "18.2.45",
		"@types/react-dom": "18.2.18",
		"@unocss/preset-mini": "0.58.0",
		"@unocss/reset": "0.58.0",
		"@vitejs/plugin-react-swc": "3.5.0",
		"eslint-config-custom": "workspace:*",
		"picocolors": "1.0.0",
		"stylelint": "16.0.2",
		"tsc-alias": "1.8.8",
		"tsconfig": "workspace:*",
		"tsx": "4.7.0",
		"unocss": "0.58.0",
		"vite": "5.0.10",
		"vite-plugin-pwa": "0.17.4",
		"vite-tsconfig-paths": "4.2.2",
		"workbox-window": "7.0.0"
	}
}
